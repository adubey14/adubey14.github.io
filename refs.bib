}

@inproceedings{Ghosh2019,
  author    = {Purboday Ghosh and Scott Eisele and Abhishek Dubey and Mary Metelko and Istv{\'{a}}n Madari and P{\'{e}}ter V{\"{o}}lgyesi and Gabor Karsai},
