@inproceedings{Dubey2010a,
  author    = {Abhishek Dubey and Gabor Karsai and R{\'{o}}bert Keresk{\'{e}}nyi and Nagabhushan Mahadevan},
  title     = {A Real-Time Component Framework: Experience with {CCM} and {ARINC-653}},
